<!DOCTYPE html>
<html>
  <head>
    <title>Maps</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  
    <%= javascript_include_tag 'https://maps.googleapis.com/maps/api/js?key='+Rails.application.credentials.google_maps_api_key %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= yield(:head_tags) %>
  </head>

  <body>
  <% if current_user %>
    <%= link_to('logout', destroy_user_session_path, :method => :delete) %> |
    <%= link_to('change password', edit_registration_path(:user)) %>
  <% else %>
    <%= link_to('register', new_registration_path(:user)) %> |
    <%= link_to('login', new_session_path(:user)) %>
  <% end %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
